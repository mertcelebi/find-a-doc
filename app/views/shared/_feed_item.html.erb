<li id="<%= feed_item.id %>">
  <span class="content">
    <%= link_to "You", feed_item.user %>
    <%= p "searched for" %>
    <%= feed_item.state %>
    <%= p "in" %>
    <%= feed_item.address %>
    <%= feed_item.city %>
    <%= feed_item.state %>
    <%= p "-" %>
    <%= feed_item.zipcode %>
  </span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
   <% if current_user?(feed_item.user) %>
    <%= link_to "View Results", feed_item, method: :delete,
                                     data: { confirm: "Are you sure?" },
                                     title: feed_item.zipcode %>
    <%= link_to "|", "#" %>
    <%= link_to "Delete", feed_item, method: :delete,
                                     data: { confirm: "Are you sure?" },
                                     title: feed_item.zipcode %>
  <% end %>
</li>